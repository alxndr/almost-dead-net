{
  "name": "almost-dead-net",
  "homepage": "http://canary.almost-dead.net",
  "version": "2.0.0",
  "repository": "https://github.com/alxndr/almost-dead-net",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "echo building... && GIT_SHA=$(git rev-parse --verify --quiet) react-scripts build",
    "predeploy": "npm run build",
    "deploy": "TAG=deploy-$(date '+%F-%H%M%S-%Z') && gh-pages --dotfiles --branch gh-pages --dist build --tag $TAG && echo cleaning up... && gh-pages-clean",
    "pretest": "npm run lint",
    "test": "react-scripts test",
    "lint": "echo linting... && eslint src/ && echo Successfully linted JS",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@sentry/browser": "^5.26.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "classnames": "^2.2.6",
    "dompurify": "^2.0.12",
    "logrocket": "^1.0.10",
    "papaparse": "^5.2.0",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-autosuggest": "^10.0.2",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-tippy": "^1.4.0",
    "urlcat": "^2.0.4"
  },
  "devDependencies": {
    "eslint": "^7.4.0",
    "gh-pages": "^3.1.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "react-app",
    "ignorePatterns": [
      "serviceWorker.js",
      "setupTests.js"
    ],
    "rules": {
      "semi": [
        "warn",
        "never"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
